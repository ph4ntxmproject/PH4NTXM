#!/bin/sh
set -e
export DEBIAN_FRONTEND=noninteractive

apt-get purge -y \
  gcc g++ \
  binutils \
  build-essential \
  make \
  libc6-dev linux-libc-dev \
  dpkg-dev pkg-config \
  autoconf automake libtool \
  || true

apt-get autoremove -y --purge || true
apt-get clean || true
rm -rf /var/lib/apt/lists/*
rm -rf /var/lib/man-db/*
